{
  "name": "sheeva",
  "version": "0.1.0",
  "description": "Concurrent Automation Test Runner",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "main": "src/index.js",
  "bin": "src/cli.js",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/sheeva.git"
  },
  "scripts": {
    "code": "npm run lint-all && npm run circular",
    "lint-all": "eslint src test rollup.config.js",
    "lint-staged": "lint-staged",
    "lint-spellcheck": "eslint src -c .eslintrc-spellcheck --no-eslintrc",
    "circular": "madge --circular --warning src",
    "tt": "node src-stable/cli --config=test/run/node/sheeva.config.js",
    "ttdev": "node src/cli --config=test/run/node/sheeva.config.js",
    "test": "npm run tt -- --target=node-sync --break-on-error --timeout=0",
    "td": "npm run ttdev -- --target=node-sync --break-on-error --timeout=0",
    "test-append": "SHEEVA_APPEND=true npm run tt",
    "test-runtyper": "SHEEVA_DIR=src-instrumented npm run tt",
    "norep": "npm t -- --reporters=",
    "ci": "npm run code && npm run ttdev",
    "debug": "npm t -- --reporters=./test/run/helpers/debug-reporter.js",
    "debug-dev": "npm run test-dev -- --reporters=./test/helpers/debug-reporter.js",
    "build": "webpack --config webpack.config.js",
    "build-test": "npm run build-test-win",
    "build-test-win": "webpack --config test/run/browser/window/webpack.config.js",
    "build-test-ww": "webpack --config test/run/browser/web-workers/webpack.config.js",
    "devtool": "devtool src-stable/cli -- --config=test/run/node/sheeva.config.js --target=node-sync --reporters=./test/helpers/debug-reporter.js --timeout=0",
    "precommit": "npm run lint-staged && npm run circular && npm run tt -- --no-only",
    "prepush": "npm run code && npm run ttdev -- --no-only && npm run copy-src",
    "copy-src": "rm -rf src-stable && cp -R src/ src-stable/",
    "runtyper": "npm run instrument && npm run test-runtyper",
    "instrument": "babel src -d src-instrumented --source-maps",
    "cores": "sysctl -n hw.ncpu"
  },
  "lint-staged": {
    "{src,test}/**/*.js": "eslint",
    "webpack.config.js": "eslint"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "commander": "^2.9.0",
    "glob": "^7.1.2",
    "promise.prototype.finally.err": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-runtyper": "^0.2.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^3.19.0",
    "eslint-plugin-spellcheck": "0.0.8",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.2",
    "lodash.mergewith": "^4.6.0",
    "madge": "^1.6.0",
    "object-path": "^0.11.4",
    "sheeva-reporter-console": "github:vitalets/sheeva-reporter-console",
    "source-map-support": "^0.4.15",
    "unexpected": "^10.29.0",
    "webpack": "^2.6.1"
  },
  "keywords": [
    "webdriver",
    "selenium",
    "appium",
    "automation",
    "test",
    "test-runner",
    "concurrency"
  ],
  "license": "MIT"
}
