{
  "name": "sheeva",
  "version": "0.1.0",
  "description": "Concurrent Automation Test Runner",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "main": "src/index.js",
  "bin": "src/cli.js",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/sheeva.git"
  },
  "scripts": {
    "code": "npm run lint-all && npm run circular",
    "lint-all": "eslint src test",
    "lint-staged": "lint-staged",
    "lint-spellcheck": "eslint src -c .eslintrc-spellcheck --no-eslintrc",
    "circular": "madge --circular --warning src",
    "tt": "node src-stable/cli --config=test/run/node/sheeva.config.js",
    "ttdev": "node src/cli --config=test/run/node/sheeva.config.js",
    "test": "npm run tt -- --target=sync-target --break-on-error --timeout=0",
    "test-dev": "npm run ttdev -- --target=sync-target --break-on-error --timeout=0",
    "test-append": "SHEEVA_APPEND=true npm run tt",
    "test-runtyper": "SHEEVA_DIR=src-instrumented npm run tt",
    "norep": "npm t -- --reporters=",
    "ci": "npm run code && npm run ttdev",
    "debug": "npm t -- --reporters=./test/run/debug-reporter.js",
    "debug-dev": "npm run test-dev -- --reporters=./test/run/debug-reporter.js",
    "build": "rollup -c rollup.config.js",
    "build-stable": "SHEEVA_DIR=src-stable rollup -c rollup.config.js",
    "build-tests": "npm run build-specs && npm run build-tests-config && npm run copy-tests-html && npm run copy-tests-data && npm run build-web-worker",
    "build-specs": "rollup -c test/run/browser/specs.rollup.js",
    "build-web-worker": "rollup -c test/run/browser/web-worker.rollup.js",
    "build-tests-config": "rollup -c test/run/browser/sheeva.rollup.js",
    "copy-tests-html": "cp -rf test/run/browser/index.html dist/test/",
    "copy-tests-data": "cp -rf test/data dist/test/",
    "devtool": "devtool src-stable/cli -- --config=test/run/node/sheeva.config.js --target=sync-target --reporters=./test/debug-reporter.js --timeout=0",
    "precommit": "npm run lint-staged && npm run circular && npm run tt -- --no-only",
    "prepush": "npm run code && npm run ttdev -- --no-only && npm run copy-src",
    "copy-src": "rm -rf src-stable && cp -R src/ src-stable/",
    "runtyper": "npm run instrument && npm run test-runtyper",
    "instrument": "babel src -d src-instrumented --source-maps",
    "cores": "sysctl -n hw.ncpu"
  },
  "lint-staged": {
    "{src,test}/**/*.js": "eslint"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "commander": "^2.9.0",
    "glob": "^7.1.2",
    "promise.prototype.finally.err": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-runtyper": "^0.2.2",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^3.19.0",
    "eslint-plugin-spellcheck": "0.0.8",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.2",
    "madge": "^1.6.0",
    "object-path": "^0.11.4",
    "rollup": "^0.41.6",
    "rollup-analyzer": "^1.0.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.3.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-string": "^2.0.2",
    "sheeva-reporter-console": "github:vitalets/sheeva-reporter-console",
    "source-map-support": "^0.4.15",
    "treeify": "^1.0.1",
    "unexpected": "^10.29.0"
  },
  "keywords": [
    "webdriver",
    "selenium",
    "appium",
    "automation",
    "test",
    "test-runner",
    "concurrency"
  ],
  "license": "MIT"
}
